{
    "description": "CRO Excel History",
    "name": "chemaclass/cro-excel-history",
    "type": "project",
    "license": "MIT",
    "keywords": ["php", "excel", "history"],
    "require": {
        "php": ">=7.4",
        "thecodingmachine/safe": "^1.3",
        "gacela-project/gacela": "^0.10.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/var-dumper": "^5.4",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.17",
        "phpstan/phpstan": "^1.3",
        "friendsofphp/php-cs-fixer": "^3.4",
        "thecodingmachine/phpstan-safe-rule": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test-all": [
            "@test-quality",
            "@test-phpunit"
        ],
        "test-quality": [
            "@csrun",
            "@psalm",
            "@phpstan"
        ],
        "test-phpunit": "./vendor/bin/phpunit",
        "test-coverage": "XDEBUG_MODE=coverage ./vendor/bin/phpunit --coverage-html coverage",
        "psalm": "./vendor/bin/psalm",
        "phpstan": "./vendor/bin/phpstan analyze -c phpstan.neon src",
        "csfix": "./vendor/bin/php-cs-fixer fix",
        "csrun": "./vendor/bin/php-cs-fixer fix --dry-run"
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    }
}
